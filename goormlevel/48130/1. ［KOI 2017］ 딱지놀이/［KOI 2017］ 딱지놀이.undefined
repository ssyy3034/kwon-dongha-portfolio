import sys

input = sys.stdin.readline
N = int(input())

for _ in range(N):
	line_a = list(map(int,input().split()))
	a_cards = line_a[1:]

	line_b = list(map(int,input().split()))
	b_cards = line_b[1:]

	a_score = [a_cards.count(4),a_cards.count(3),a_cards.count(2),a_cards.count(1)]
	b_score = [b_cards.count(4),b_cards.count(3),b_cards.count(2),b_cards.count(1)]

	if a_score > b_score:
		print("A")
	elif a_score < b_score:
		print("B")
	else:
		print("D")